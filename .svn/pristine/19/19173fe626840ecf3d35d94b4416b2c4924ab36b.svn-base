
*{ margin:0 auto; padding:0;}
body{ background:#3d8dd6;}

.img{ background:url(../img/right_bot.png) no-repeat; position:absolute; right:0; bottom:0; width:814px; height:809px;background-size:cover}
.text{ font-size:60px; color:#FFF;}
.input_01{ float:left; width:200px; height:40px; border-radius:5px 5px 5px 5px;}
.yhm{ width:100%; height:100%; border-radius:5px 5px 5px 5px; border:none; text-align:center; font-size:18px;}
.input_02{ float:left; width:150px; height:40px;  border-radius:5px 5px 5px 5px; margin-left:20px;}
.input_03{ float:left; width:120px; height:40px; border-radius:5px 0px 0px 5px; margin-left:20px;}
.input_04{ float:left; width:80px; height:40px; background:#FFF; box-sizing:border-box;  text-align:center;vertical-align:middle; background:#95d4ff; color:#FFF; font-size:16px; background-size:cover;}
.yhm_0{ width:100%; height:100%; border-radius:5px 0px 0px 5px; border:none; text-align:center;font-size:18px;}
.input_05{ float:left; width:40px; height:40px;box-sizing:border-box; padding-top:10px; text-align:center; background:#95d4ff; border-radius:0px 5px 5px 0px;cursor:pointer;}

.input_06{ float:left; width:60px; height:40px;box-sizing:border-box; padding-top:8px; text-align:center; background:#95d4ff; border-radius:5px 5px 5px 5px; margin-left:20px; color:#FFF; font-size:16px;cursor:pointer;}
.input_06:active{background:#67befa;}
